ENV=dev
REDIS_URL=redis://redis:6379/0
REDIS_URL_QUEUE=redis://redis:6379/2
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/agentic
OPENAI_API_KEY=sk-...
SERPAPI_KEY=...
S3_BUCKET=agentic-dev
JWT_PUBLIC_KEY=-----BEGIN PUBLIC KEY-----\n...\n-----END PUBLIC KEY-----
OTEL_EXPORTER_OTLP_ENDPOINT=
OTEL_SERVICE_NAME=agentic-backend
LOG_LEVEL=INFO
PORT=8080

# Optional: cost table override (per 1k tokens)
COST_TABLE_JSON={"gpt-4o-mini":{"in":0.15,"out":0.60}}
# Optional: local OpenAI-compatible endpoint (vLLM/Ollama)
LOCAL_OPENAI_BASE_URL=
LOCAL_OPENAI_API_KEY=local-key

# Rate limits (fixed window per minute)
RATE_LIMIT_USER_PER_MIN=60
RATE_LIMIT_TENANT_PER_MIN=600

# Idempotency (seconds)
IDEMPOTENCY_TTL_SECONDS=86400

# Concurrency per outbound host (tools)
MAX_CONCURRENCY_PER_HOST=8

# Webhook signature
WEBHOOK_HMAC_SECRET=change-me
WEBHOOK_MAX_ATTEMPTS=6

# OpenTelemetry (Tempo) â€” leave blank to disable exporter in local dev
OTEL_EXPORTER_OTLP_ENDPOINT=http://tempo:4317
OTEL_EXPORTER_OTLP_PROTOCOL=grpc

S3_BUCKET=your-bucket
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1
ARTIFACTS_DIR=/data/artifacts
OPENAI_API_KEY=...   # for LLM outline
